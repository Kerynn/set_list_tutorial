<h1>Artists</h1>
<% @artists.each do |artist| %>
  <p>
    <h3><%= artist.name %></h3>
    <%= button_to "Edit #{artist.name}", "/artists/#{artist.id}/edit", method: :get %>
    <%= button_to "Delete", "/artists/#{artist.id}", method: :delete %>
  </p>
<% end %>

<%= link_to "New Artist", "/artists/new" %>

